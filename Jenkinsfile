    agent any

    stages {
        stage('Build code') {
            steps {
                echo 'Building the project...'
            }
        }

        stage('Test code') {
            steps {
                echo 'Running tests...'
            }
        }

        stage('Deploy code') {
            steps {
                echo 'Deploying application...'
            }
        }
    }
}
